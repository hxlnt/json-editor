{
    "id": {
        "type": "integer",
        "minimum": 0
    },
    "title": {
        "native": {
            "type": "string",
            "default": "あにめらんど"
        },
        "romanized": {
            "type": "string",
            "default": "Anime Land"
        }
    },
    "publisher": {
        "type": "array"
    },
    "developer": {
        "type": "array"
    },
    "releasedate": {
        "year": {
            "type": "integer",
            "minimum": 1900,
            "default": 1995
        },
        "month": {
            "type": "integer",
            "minimum": 1,
            "maximum": 12
        },
        "day": {
            "type": "integer",
            "minimum": 1,
            "maximum": 31
        }
    },
    "mediaformat": {
        "type": "string",
        "enum": ["cartridge", "3.5-in. disk", "5.25-in. disk", "FDS disk", "CD-ROM"]
    },
    "platform": {
        "type": "string",
        "enum": ["Casio Loopy","NES","SNES"]
    }
}
